/* pages/wait/wait.wxss */
.custom{
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 1rem;
}
.page{
  width: 100vw;
  height: 100vh;
  box-sizing: border-box;
  overflow: hidden
}
image{
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.b{
  position: fixed;
  height: 100%;
  width: 100%;
  display:flex;
  flex-direction:column;
  align-items:center;/*垂直居中*/
  justify-content: center;/*水平居中*/
}